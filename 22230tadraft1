<TopicAssessment paperSize="A4" language="English" version="2.19.0">
  <QuestionsGroup title="5.01 Quadratic relationships FLUENCY">
    <Question
      linkedExamples={['1']}
      label="4"
      conversionMetadata="20854 849155592 2020-12-31"
    >
      <Text>
        Which of these equations represent a quadratic relationship between{' '}
        <Latex expression="x" /> and <Latex expression="y" />?
      </Text>

      <Parts columns={2}>
        <Part>
          {' '}
          <Text>
            <Latex expression="2 x -  3 y - 2 = 0" />
          </Text>
          {/* #answer <Text>No</Text> */}
        </Part>
        <Part>
          {' '}
          <Text>
            <Latex expression="y =  2 x^{2} -  3 x - 2" />
          </Text>
          {/* #answer <Text>Yes</Text> */}
        </Part>
        <Part>
          <Text>
            <Latex expression="y =  5 x^{3} - x^{2}" />
          </Text>
          {/* #answer <Text>No</Text> */}
        </Part>
        <Part>
          <Text>
            <Latex expression="y =  2 x - 3" />
          </Text>
          {/* #answer <Text>No</Text> */}
        </Part>
        <Part>
          {' '}
          <Text>
            <Latex expression="y = \left(x^{2} - 3\right)^{2}" />
          </Text>
          {/* #answer <Text>No</Text> */}
        </Part>
        <Part>
          <Text>
            <Latex expression="y = 1 -  3 x -  5 x^{2}" />
          </Text>
          {/* #answer <Text>Yes</Text> */}
        </Part>
        <Part>
          {' '}
          <Text>
            <Latex expression="6 x^{2} -  2 x + 7" />
          </Text>
          {/* #answer 
                                      
                                     <Text>No</Text>  
                                     */}
        </Part>
        <Part>
          {' '}
          <Text>
            <Latex expression="y=\sqrt{x^{2} +  3 x - 4}" />
          </Text>
          {/* #answer 
                                      
                                     <Text>No</Text>  
                                     */}
        </Part>
        <Part>
          {' '}
          <Text>
            <Latex expression="y=\dfrac{1}{4 x^{2} -  6 x - 6}" />
          </Text>
          {/* #answer 
                                      
                                     <Text>No</Text>  
                                     */}
        </Part>
        <Part>
          {' '}
          <Text>
            <Latex expression="y=x^{2} + \dfrac{1}{x}" />
          </Text>
          {/* #answer 
                                      
                                     <Text>No</Text>  
                                     */}
        </Part>
        <Part>
          <Text>
            <Latex expression="y = x^{2} +  4 x" />
          </Text>
          {/* #answer <Text>Yes</Text> */}
        </Part>
        <Part>
          <Text>
            <Latex expression="y = \dfrac{5}{x^{2}}" />
          </Text>
          {/* #answer <Text>No</Text> */}
        </Part>
        <Part>
          {' '}
          <Text>
            <Latex expression=" 1 - x^{2}=y" />
          </Text>
          {/* #answer <Text>Yes</Text> */}
        </Part>
        <Part>
          {' '}
          <Text>
            <Latex expression="y = \left(x^{2} - 1\right)^{3}" />
          </Text>
          {/* #answer <Text>No</Text> */}
        </Part>
        <Part>
          <Text>
            <Latex expression="y = x^{2} + \dfrac{2}{x}" />
          </Text>
          {/* #answer <Text>No</Text> */}
        </Part>
        <Part>
          {' '}
          <Text>
            <Latex expression="y =  2 x^{2}" />
          </Text>
          {/* #answer <Text>Yes</Text> */}
        </Part>
      </Parts>
    </Question>
  </QuestionsGroup>

  <QuestionsGroup title="5.01 QUADRATIC RELATIONSHIPS PROBLEM-SOLVING">
    <Question moveToNextPrintedPage label="15">
      <Text>Consider the table shown:</Text>
      <TabularData
        rowTitles={['x', 'y']}
        rowTitlesAs="latex"
        rowData={[
          ['-2', '-1', '0', '1', '2'],
          ['16', '4', '0', '4', '16'],
        ]}
        rowDataAs="latex"
      />

      <Parts columns={1}>
        <Part>
          Describe the pattern of the output in words.
          {/* #answer  <Text>
            To find the <LatexTextAtom expression="y" text="-values" /> find the
            square of <LatexTextAtom expression="x" text="-values" />, and then
            multiply the result by <Latex expression="4" />.
          </Text> */}
        </Part>
        <Part>
          Determine if the rule would be a linear or quadratic relationship.
          {/* #answer Quadratic */}
        </Part>
        <Part>
          Write the rule that relates <Latex expression="x" /> and{' '}
          <Latex expression="y" />.
          {/* #answer <Latex expression="y=4x^{2}" /> */}
        </Part>
      </Parts>
    </Question>
  </QuestionsGroup>

